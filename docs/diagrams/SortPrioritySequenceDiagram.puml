@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant "sp:SortPriorityCommand" as SortPriorityCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "model:Model" as Model MODEL_COLOR
participant "addressBook:AddressBook" as AddressBook MODEL_COLOR
participant "uniqueClientList:UniqueClientList" as UniqueClientList MODEL_COLOR
end box

actor User
User -> SortPriorityCommand : execute(model)
activate SortPriorityCommand

SortPriorityCommand -> Model : sortClientsByPriority()
activate Model

Model -> AddressBook : getUniqueClientList()
activate AddressBook
AddressBook --> Model : uniqueClientList
deactivate AddressBook

Model -> UniqueClientList : sort()
activate UniqueClientList
UniqueClientList --> Model
deactivate UniqueClientList

Model --> SortPriorityCommand
deactivate Model

SortPriorityCommand --> User : CommandResult(MESSAGE_SORTED_SUCCESS)
deactivate SortPriorityCommand

@enduml 